<SalesMano>:
    FloatLayout:
        # Geral
        canvas:
            Rectangle:
                size:self.size
                pos:self.pos
                source:'icones/bg1.png'

        #Meio
        ScrollView:
            size_hint:1,0.78
            pos_hint:{'right':1,'top':0.9}
            GridLayout:
                cols:1
                id:lista_vendas
                size_hint_y: None
                row_default_height:"90dp"
                height:self.minimum_height
                row_force_default:True
                spacing:2,2


        #Barra inferior
        GridLayout:
            pos_hint: {'right':1 , 'top': 0.12}
            size_hint: 1,0.12 
            cols:3
            canvas:
                Color:
                    rgb:(0, 0.556, 0.611)
                Rectangle:
                    pos:self.pos
                    size:self.size
            ImageButton:
                on_release:app.mudar_pagina('homepage')
                source:'icones/voltar.png'
            Label:
            Label: